      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="[Magento].[SqlCommand]">"SELECT  
	IFNULL(CAST(so.customer_id + 1 AS SIGNED),1) AS ClienteID, -- DimCLiente
    CAST(sii.product_id AS SIGNED) AS ProductoID, -- DimProducto
    IFNULL(CAST(so.applied_rule_ids + 1 AS SIGNED),1) AS PromocionID,  -- DimPromocion,
    CAST(si.order_id AS SIGNED) AS OrdenID,   -- DimOrden
    CONVERT(CONCAT(YEAR(so.created_at), LPAD(MONTH(so.created_at), 2, '0'), LPAD(DAY(so.created_at), 2, '0')), SIGNED) AS FechaOrden,
    CONVERT(CONCAT(YEAR(so.updated_at), LPAD(MONTH(so.updated_at), 2, '0'), LPAD(DAY(so.updated_at), 2, '0')), SIGNED) AS FechaFacturacionEnvio,
    -- so.updated_at AS FechaEnvio,
    sii.qty AS Cantidad,
    sii.base_price AS precio_unitario_regular,
    sii.base_row_total AS precio_extendido,
    IFNULL(sii.discount_amount,0.0) AS cantidad_descuento,
    (sii.base_row_total-IFNULL(sii.discount_amount,0.0)) AS precio_venta_final   
FROM magento.sales_invoice_item sii
	INNER JOIN magento.sales_invoice si ON sii.parent_id=si.entity_id
    INNER JOIN magento.sales_order so ON si.order_id=so.entity_id
  WHERE CAST(so.updated_at AS CHAR) &gt; '"+(DT_WSTR, 100)@[User::UltimaFechaEjecucion]+"'"</DTS:PropertyExpression>
